{
  "version": 2,
  "builds": [
    {
      "src": "Build/**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/Build/*.gz",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    },
    {
      "source": "/Build/*.br",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    },
    {
      "source": "/Build/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
